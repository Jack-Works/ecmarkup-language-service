{
    "name": "ecmarkup-language-server",
    "private": true,
    "packageManager": "pnpm@7.17.1",
    "description": "Language server and editor plugins for ecmarkup",
    "author": "Jack Works",
    "license": "MIT",
    "scripts": {
        "watch": "tsc -b . -w"
    },
    "devDependencies": {
        "prettier": "^2.8.1",
        "typescript": "5.0.0-dev.20221210"
    },
    "pnpm": {
        "overrides": {
            "typescript": "5.0.0-dev.20221210",
            "grammarkdown": "workspace:*"
        },
        "packageExtensions": {
            "grammarkdown": {
                "dependencies": {
                    "@esfx/disposable": "*",
                    "jest-matcher-utils": "*"
                }
            }
        }
    }
}
