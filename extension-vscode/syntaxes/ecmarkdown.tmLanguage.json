        "AttributeList": {
            "begin": "(\\[)(?!\\[)",
            "end": "(\\])(?!\\])",
            "patterns": [
                {
                    "name": "string.quoted",
                    "begin": "\"",
                    "end": "\""
                },
                {
                    "name": "punctuation.separator.key-value",
                    "match": "="
