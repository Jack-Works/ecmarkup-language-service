                        "1": {
                            "name": "meta.bracket.square.access"
                        }
                    },
                    "endCaptures": {
                        "1": {
                            "name": "meta.bracket.square.access"
                        }
                    }
                },
                {
                    "name": "keyword.control.assert",
                    "match": "(?i)Assert:|!"
                },
                {
                    "name": "keyword.control.trycatch",
                    "match": "(?i)\\b(Throw|\\?)\\b"
                },
                {
                    "name": "keyword.control.flow",
                    "match": "(?i)\\b(If|Else if|Else|then|is not present|return|repeat|while)\\b"
