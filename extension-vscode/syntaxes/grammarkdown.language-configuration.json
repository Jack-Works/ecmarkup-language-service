{
    "comments": {
        "blockComment": ["/*", "*/"],
        "lineComment": "//"
    },
    "brackets": [["(", ")"], ["[", "]"], ["{", "}"]],
    "autoClosingPairs": [["{", "}"], ["[", "]"], ["(", ")"], ["`", "`"]],
    "surroundingPairs": [["(", ")"], ["[", "]"], ["{", "}"], ["`", "`"]],
    "indentationRules": {
        "increaseIndentPattern": "^(?!\\/\\/).*:\\s*(one of)?\\s*$",
        "decreaseIndentPattern": "^this_never_matches$"
    }
}
